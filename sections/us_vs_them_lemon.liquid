        <img data-sizes="auto" data-srcset="{% include 'lazy_src', image: section.settings.fourth_bottle_hover %}" alt="{{section.settings.fourth_bottle_image_alt}}" class="hover-img animation lazyload">
			</div>
			<table border="0" cellpadding="0" cellspacing="0" style="width: 100%;">
				<tr>
					<th colspan="2">{{section.settings.fourth_title}}</th>
				</tr>
				<tr>
					<td width="80%" class="text-left">Calories</td>
					<td width="20%">{{section.settings.fourth_calories}}</td>
				</tr>
				<tr>
					<td width="80%" class="text-left">Sodium</td>
					<td width="20%">{{section.settings.fourth_sodium}}</td>
				</tr>
				<tr>
					<td width="80%" class="text-left">Potassium</td>
					<td width="20%">{{section.settings.fourth_potassium}}</td>
				</tr>
				<tr>
					<td width="80%" class="text-left">Sugar</td>
					<td width="20%">{{section.settings.fourth_sugar}}</td>
				</tr>
				<tr>
					<td width="80%" class="text-left">Added Sugar</td>
					<td width="20%">{{section.settings.fourth_added_sugar}}</td>
				</tr>
				<tr>
					<td width="80%" class="text-left">No Artificial Ingredients</td>
					<td width="20%">
						{% if section.settings.fourth_no_artificial_ingredients == "no" %}
            <img src="{{ "cross.svg" | asset_url }}" alt="no" class="cross-icon">
	          {% else %}
	            <img src="{{ "check.svg" | asset_url }}" alt="yes" class="check-icon">
	          {% endif %}
					</td>
				</tr>
			</table>
		</div>

	</div>
</section>



