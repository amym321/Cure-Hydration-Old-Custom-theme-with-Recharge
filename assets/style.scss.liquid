/******** tooltip *******/

.swatch .tooltip {text-align:center; background:{{settings.content_main_color}}; color:#fff; bottom:100%; padding: 4px; font-size: 11px; display:none; {% if settings.enable_color_swatches == true %}display: block;{% endif %} position:absolute; width:90px; left: 50%; margin-left:-47px; margin-bottom:15px; filter:alpha(opacity=0); -khtml-opacity: 0; -moz-opacity: 0; opacity:0; visibility:hidden; -webkit-transform: translateY(10px); -moz-transform: translateY(10px); -ms-transform: translateY(10px); -o-transform: translateY(10px); transform: translateY(10px); -webkit-transition: all .25s ease-out; -moz-transition: all .25s ease-out; -ms-transition: all .25s ease-out; -o-transition: all .25s ease-out; transition: all .25s ease-out; z-index: 10000;}
.swatch .tooltip:before {bottom:-20px; content:" "; display:block; height:20px; left:0; position:absolute; width:100%;}
