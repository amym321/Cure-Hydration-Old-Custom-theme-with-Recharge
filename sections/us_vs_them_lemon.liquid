	  $('table').removeClass('col-4-hover');
	});
	$(function(){
    var itemsSlider = $('#shopify-section-{{ section.id }} .mobile-content');
    itemsSlider.slick({
      autoplay: false,
      dots: true,
      arrows: true,
      prevArrow: '<i class="fa fa-angle-left" aria-hidden="true"></i>',
      nextArrow: '<i class="fa fa-angle-right" aria-hidden="true"></i>',
      fade: false,
      pauseOnHover: true,
      speed: 1000,
