<style>
  #shopify-section-{{section.id}} { {% if section.settings.bg_img %} background-image: url({{section.settings.bg_img | img_url: 'master'}});background-size: cover;background-position: center;background-repeat: no-repeat;{% endif %}padding:1em;}
	
	#shopify-section-{{section.id}} .us-vs-them-section{background-color: transparent;}

	#shopify-section-{{section.id}} .mobile-content{display: none;}

	#shopify-section-{{section.id}} .title-container{text-align: left;border-bottom: 1px solid #eef2fe;padding-bottom: 0.5em;margin-bottom: 2em;}
	#shopify-section-{{section.id}} .title-container img{width: 335px;max-width: 90%;}
	
	#shopify-section-{{section.id}} table th{line-height: 15px;padding:10px 0;}
	#shopify-section-{{section.id}} table th:not(.empty){background-color: #eef2fe;color: #6085fa;font-size: 0.7em;font-weight: 800;}
	#shopify-section-{{section.id}} .desktop-content table tr td{width: calc(100%/5);}
	#shopify-section-{{section.id}} table tr td{border: 1px solid #eef2fe;text-align: center;font-family: GT Pressura Mono, sans-serif;font-size: 0.7em;color: #eef2fe;line-height: 15px;padding:10px 0;}
	#shopify-section-{{section.id}} table tr td.text-left{text-align: left;padding-left: 10px;}
	#shopify-section-{{section.id}} .images-row td{border: none;}
	#shopify-section-{{section.id}} table tr td .cross-icon,
	#shopify-section-{{section.id}} table tr td .check-icon{position: relative;top: 3px;max-width: 21px;margin: auto;}
	#shopify-section-{{section.id}} .image-container{position: relative;padding-top: 160px;}
	#shopify-section-{{section.id}} .image-container img{position: absolute;position: absolute;left: 50%;top: 50%;transform: translate(-50%,-50%);max-width: 65px;}
	#shopify-section-{{section.id}} .image-container .hover-img{opacity: 0;}
	
	.isNotMobile #shopify-section-{{section.id}} .desktop-content table.col-1-hover .col-1.image-container .hover-img{opacity: 1;z-index: 2;}
	.isNotMobile #shopify-section-{{section.id}} .desktop-content table.col-1-hover .col-1:not(.no-bg-hover){background-color: #809df9;}
	.isNotMobile #shopify-section-{{section.id}} .desktop-content table.col-2-hover .col-2.image-container .hover-img{opacity: 1;z-index: 2;}
	.isNotMobile #shopify-section-{{section.id}} .desktop-content table.col-2-hover .col-2:not(.no-bg-hover){background-color: #809df9;}
	.isNotMobile #shopify-section-{{section.id}} .desktop-content table.col-3-hover .col-3.image-container .hover-img{opacity: 1;z-index: 2;}
	.isNotMobile #shopify-section-{{section.id}} .desktop-content table.col-3-hover .col-3:not(.no-bg-hover){background-color: #809df9;}
	.isNotMobile #shopify-section-{{section.id}} .desktop-content table.col-4-hover .col-4.image-container .hover-img{opacity: 1;z-index: 2;}
	.isNotMobile #shopify-section-{{section.id}} .desktop-content table.col-4-hover .col-4:not(.no-bg-hover){background-color: #809df9;}

	#shopify-section-{{section.id}} .slick-slider .slick-arrow{color: #eef2fe;font-weight: 400;font-size: 40px;top: 18%;}
	#shopify-section-{{section.id}} .slick-slider .slick-arrow.fa-angle-right{right: 0;}
	#shopify-section-{{section.id}} .slick-dots li{margin:0 10px;}
	#shopify-section-{{section.id}} .slick-dots li button{height: 10px;width: 10px;border: 1px solid #eef2fe;background-color: {{settings.content_main_color}};}
	#shopify-section-{{section.id}} .slick-dots li.slick-active button{background-color: #eef2fe;}


  @media all and (max-width: 600px){
		#shopify-section-{{section.id}} .us-vs-them-section{padding: 2em 1em 3em;}

  	#shopify-section-{{section.id}} .desktop-content{display: none;}
  	#shopify-section-{{section.id}} .mobile-content{display: block;}
  	#shopify-section-{{section.id}} .mobile-content .image-container{margin-bottom: 1em;}
  	#shopify-section-{{section.id}} table tr td .cross-icon,
  	#shopify-section-{{section.id}} table tr td .check-icon{top: 1px;max-width: 18px;}

		#shopify-section-{{section.id}} .slick-dots{bottom: -2.5em;}
  }
</style>

