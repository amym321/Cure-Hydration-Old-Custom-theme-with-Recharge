/*
/******************************** HEADER *****************************
*/

.header {color: {{settings.header_text_color}}; position: relative;}

#header {width: 100%; text-align: center; background: {{settings.header_color}}; border-bottom: 1px solid {{settings.header_border_color}}; position: relative;}
.header .logo {line-height: 100%;}
.header .logo img {max-width: {{settings.logo_max_width}};}
.header .main-content {position: relative;}
.top-utils ul {list-style-type: none;}
.top-utils li {margin-left: 1.5em;font-size: 0.7em;font-weight: 500;text-transform: uppercase;letter-spacing: 1.73px;}
.top-utils i {margin-right: 0.5em;}
.top-utils .search-icon{max-width: 20px;}
.top-header {font-size: 0.8em; padding-top: 1em;}
.top-header * {line-height: 100%;}
.top-header .social-links li {display:inline-block; margin: 0; margin-right: 1.5em;}
.top-header .social-links li:hover i {color: {{settings.content_main_color}};}

{% if settings.header_behavior == "fixed" %}
#header {position: fixed; z-index: 100;}
{% endif %}

{% if settings.over_carousel %}
@media all and (min-width: 901px){
  #header.over-carousel {color: #fff; position: absolute; z-index: 100; background: transparent; border: none;}
  #header.over-carousel .main-menu .submenu {background: transparent;}
}
{% endif %}

{% if settings.drop_header %}
/**** header drop ****/
.header-drop {position: fixed; width: 100%; box-shadow: 0 0 10px rgba(0,0,0,0.1); background-color: {{ settings.header_color }}; top: -80px; z-index: 999;}
.header-drop.scroll {top: 0; }
.header-drop .main-content {position: relative;}
.header-drop .logo {line-height: 100%;}
.header-drop .logo img {max-width: {{settings.drop_logo_max_width}}; max-height: 40px;}
/*.header-drop .top-utils li span:not(.count) {display: none;}*/
.header-drop .main-menu {margin-left: 2em;}
.header-drop .main-menu ul {text-align: center;}
.header-drop .main-menu li a {padding: 1.5em 0;}
{% endif %}

{% if settings.show_newsletter_header or settings.show_search_header %}
/**** full screen search and mailing ****/
.search-mailing .form .input-icon {position: absolute; right: 0; font-size: 30px; top: 22px; width: auto; height: auto; padding: 0;}
.search-mailing {position: fixed; z-index: 4000; width: 100%; height: 100%; display: none; top: 0; left: 0; background: rgba({{settings.search_mailing_bg}});}
.search-mailing i.lnr-cross {position: fixed; display: none; z-index: 99997; top: 30px; right: 30px; color: {{settings.search_mailing_text}}; font-size: 35px; cursor: pointer}
.search-mailing .form {display: none; position: fixed; z-index: 99997; width: 50%; margin-left: -25%; left: 50%; height: 80px; top: 30%; margin-top: -40px; }
.search-mailing .form input { color: {{settings.search_mailing_text}}; font-size: 50px; padding: 0 0 10px 7px; width: 100%; border: 0; background: transparent; -webkit-appearance: none; outline: 0; font-weight: 100;}
.search-mailing .form ::-webkit-input-placeholder {color: {{settings.search_mailing_text}};}
.search-mailing .form :-moz-placeholder {color: {{settings.search_mailing_text}}; opacity: 1;}
.search-mailing .form ::-moz-placeholder {color: {{settings.search_mailing_text}}; opacity: 1;}
.search-mailing .form :-ms-input-placeholder {color: {{settings.search_mailing_text}};}
.search-mailing .success { color: {{settings.search_mailing_text}}; font-size: 50px; padding: 0 0 10px 7px; width: 100%; line-height: 1em; border: 0; background: transparent; outline: 0; font-weight: 100;}
{% endif %}

{% if settings.show_utility_bar %}
/**** utility bar ****/
#utility-bar {background: {{settings.utility_bar_color}}; line-height: 130%; position: relative; color: {{settings.utility_bar_text_color}}; overflow: hidden; padding: 0.35em 0; text-align: center; font-size: {{settings.utility_bar_text_size}};text-transform: uppercase;font-weight: 600;letter-spacing: 2px;}
#utility-bar a:hover {color: {{settings.utility_bar_text_color}};}
#utility-bar .container {position: relative; padding: 0 40px; max-width: 2000px; height: auto !important;}
#utility-bar i {position: absolute; color: {{settings.utility_bar_close_color}}; right: 6px; top: 50%; font-size: 16px; margin-top: -9px; }
{% endif %}

/**** menu ****/

.main-menu ul {list-style-type: none;}
.main-menu li {display: inline-block; margin: 0 2.5em; letter-spacing: {{settings.menu_letter_spacing}}; text-transform: {{ settings.menu_text_transform }}; font-family: {% if settings.menu_font == "body" %}{{ settings.body_font }}{% else %}{{ settings.heading_font }}{% endif %}; font-size: {{settings.menu_font_size}}; font-weight: {{settings.menu_font_weight}};}
.main-menu li:first-child {margin-left: 0;}
.main-menu li:last-child {margin-right: 0;}
.main-menu li a {display: block; padding: 1.8em 0; font-size: .9em;}
.main-menu .arrow {display: none; margin-left: 0.5em;}

/* dropdown menus */
.main-menu .submenu {display: none; font-size: 1em; background: {{ settings.dropdown_bg_color }};}
.main-menu .has-dropdown {position: relative;}
.main-menu .has-dropdown > a > .arrow {display: inline-block;}
.main-menu .has-dropdown ul {top: 100%; left: -2em; padding: 0.5em 0; position: absolute; z-index: 30; }
.main-menu .has-dropdown li {font-size: 1em; text-align: left; display: block; margin: 0; }
.main-menu .has-dropdown li:last-child {border-bottom: 0px;}
.main-menu .has-dropdown li a {position: relative; margin: 0; min-width: 170px; padding: 0.8em 2em; display: block; border: none; /* padding-right: 0px; */}
.main-menu .has-dropdown li .arrow-right {position: absolute; right: 20px; top: 50%; margin-top: -8px; font-size: 14px; margin-left: 3px;}

/* level 3 */
.main-menu .has-dropdown ul .has-dropdown ul {top: -0.5em !important; left: 100%;}

/* menu mobile */
#menu-mobile-button {width: 2.5em; font-size: 18px; position: absolute; top: 0; right: 0; height: 100%; background: {{settings.mobile_menu_button_color}}; color: {{settings.mobile_menu_button_icon}}; display: flex; display: -webkit-flex; justify-content: center; align-items: center; }
#menu-mobile {display: none; width: 400px; left: -415px; overflow: hidden; box-shadow: 0 0 30px rgba(0,0,0,0); background-color: {{settings.mobile_menu_bg_color}}; position: fixed; top: 0; bottom: 0; z-index: 9999;}
#menu-mobile.open {left: 0; box-shadow: 0 0 30px rgba(0,0,0,0.1);}
