/* 
/******************************** RESET *****************************
*/
html, body, div, span, iframe, button, input, select, textarea, p, img, sub, sup, ol, ul, li, form, label, table, tbody, tfoot, thead, tr, th, td, article, aside, canvas, footer, header, nav, section, summary, video
{ margin: 0;  padding: 0;  border: 0;  font-size: 100%;  font: inherit;  vertical-align: baseline; }
article, aside, details, footer, header, nav, section  { display: block; }
hr { display: block; height: 1px; border: 0; border-top: 1px solid #ccc; margin: 1em 0; padding: 0; }
input, select { vertical-align: middle; }
ol { list-style-type: decimal; list-style-position: inside;}
small { font-size: 85%; }
strong { font-weight: bold; }
sub { font-size: 65%; line-height: 0; position: relative; }
sup { top: -0.5em; }
textarea { overflow: auto; }
label, input[type="buttotn"], input[type="submit"], input[type="image"], button { cursor: pointer; }
input.error, textarea.error { border-color:red }
a {color: inherit; text-decoration: none;}
.ie7 img { -ms-interpolation-mode: bicubic; }
table{  width: 100%;  border-collapse: collapse; vertical-align: middle; border-spacing: 0; }
table td, table th {vertical-align: middle; }
ul {list-style-position: inside;}
input::-ms-clear {display: none;}
input[type="text"],input[type='email'], input[type="tel"], input[type="phone"], input[type='password'], input[type="submit"], input[type="button"] {appearance:none; -moz-appearance:none; /* Firefox */ -webkit-appearance:none; /* Safari and Chrome */ border-radius: 0; background: transparent; border: 0;}
input[type="submit"], input[type="button"], button {cursor: pointer; flex-shrink: 0;}
input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill {background-color: {{settings.input_background}} !important; -webkit-box-shadow: 0 0 0px 1000px {{settings.input_background}} inset; box-shadow: 0 0 0px 1000px {{settings.input_background}} inset; color: {{settings.input_text}}; -webkit-text-fill-color: {{settings.input_text}};}
input[type=number]::-webkit-inner-spin-button, 
input[type=number]::-webkit-outer-spin-button {opacity: 1;}

/*
/******************************** BASE CONTENT STYLES *****************************
*/

html,body { background: {{settings.bg_color}}; -webkit-text-size-adjust: 100%; -webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale; -webkit-tap-highlight-color: transparent;}
{% if settings.heading_font == "'Raleway', sans-serif" or settings.body_font == "'Raleway', sans-serif" %}
/* for Raleway Numbers */
html,body {font-feature-settings: 'lnum'; -moz-font-feature-settings: 'lnum=1'; -ms-font-feature-settings: 'lnum'; -webkit-font-feature-settings: 'lnum'; -o-font-feature-settings: 'lnum';}
{% endif %}
body, html {margin: 0px; font-size: {{ settings.body_font_size }}; color: {{ settings.content_text_color}};}
p, li, input, textarea, select {font-size: 1em;}

body, p, li, input, textarea, select {font-family: {{ settings.body_font }}; font-weight: {{ settings.body_font_weight }}; font-style: {{ settings.body_font_style }}; letter-spacing: {{settings.body_font_letter_spacing}}; line-height: 170%;}
h1, h2, h3, h4, h5, h6 {margin: 0 0 1em; font-family: {{ settings.heading_font }}; font-weight: {{ settings.heading_font_weight }}; font-style: {{ settings.heading_font_style }}; text-transform: {{ settings.heading_font_transform }}; color: {{ settings.content_heading_color }}; letter-spacing: {{settings.heading_font_letter_spacing}}; line-height: 120%;  }
h1 {font-size: 2.5em;} h2 {font-size: 2.2em;} h3 {font-size: 1.75em;} h4 {font-size: 1.5em;} h5 {font-size: 1.25em;} h6 {font-size: 1em;}
p {margin: 0 0 1em 0;}

#page {background-color: {{settings.bg_color}};}
.content-wrapper {background: {{settings.bg_color}}; }
* {-moz-box-sizing:border-box; -webkit-box-sizing:border-box; box-sizing:border-box;}
.container {max-width: {{settings.container_width}}; margin: auto; padding: 0 20px; -moz-box-sizing:initial; -webkit-box-sizing:initial; box-sizing:initial; }
.container-big {max-width: 1360px; margin: auto; padding: 0 20px; -moz-box-sizing:initial; -webkit-box-sizing:initial; box-sizing:initial; }

{% if settings.header_behavior != "fixed" %}
@media all and (min-width: 901px) {
.content-wrapper {padding-top: 0 !important;}
}
{% endif %}

.shopify-challenge__container {padding: 5em 0; }

a {-webkit-transition: all 0.2s linear; -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear;}
.animation {-webkit-transition: all 0.2s linear; -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear;}
.ease-animation {-webkit-transition: all .2s ease-out; -moz-transition: all .2s ease-out; -o-transition: all .2s ease-out; transition: all .2s ease-out; }
.ease-animation-slow {-webkit-transition: all .5s ease-out; -moz-transition: all .5s ease-out; -o-transition: all .5s ease-out; transition: all .5s ease-out; }
.isNotMobile a:hover { color: {{ settings.content_main_color}} }
input, textarea{font-family: {{ settings.body_font }}; font-weight: {{ settings.body_font_weight }}; font-style: {{ settings.body_font_style }}; color: {{ settings.content_text_color}}; -webkit-transition: all 0.2s linear; -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear;}

:focus {outline:none;}
::selection {background: {{ settings.content_main_color}}; color: #fff;}
::-moz-selection {background: {{ settings.content_main_color}}; color: #fff}
::-webkit-input-placeholder {color: {{settings.placeholder_color}};}
:-moz-placeholder {color: {{settings.placeholder_color}};}
::-moz-placeholder {color: {{settings.placeholder_color}};}
:-ms-input-placeholder {color: {{settings.placeholder_color}};}

/*
******************** Lazyload ********************
*/
.lazyload, .lazyloading {-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)"; filter: alpha(opacity=0); -moz-opacity: 0; -khtml-opacity: 0; opacity: 0; -webkit-transition: opacity 0.5s ease-out; -moz-transition: opacity 0.5s ease-out; -o-transition: opacity 0.5s ease-out; -ms-transition: opacity 0.5s ease-out; transition: opacity 0.5s ease-out;}
.lazyloaded {-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)"; filter: alpha(opacity=100); -moz-opacity: 1; -khtml-opacity: 1; opacity: 1; }
[data-sizes=auto] {display: block;}

.wow {visibility: hidden;}

/**** SELECT STYLES ****/

select {appearance:none; -moz-appearance:none; /* Firefox */ -webkit-appearance:none; /* Safari and Chrome */ border-radius: 0;}
select {width: 100%; line-height: 100%; color: {{settings.input_text}}; border: {{settings.input_border}}; -webkit-border-radius: {{settings.input_radius}}; -moz-border-radius: {{settings.input_radius}}; border-radius: {{settings.input_radius}}; padding: {{settings.input_padding}};  font-size: {{settings.input_size}}; text-transform: {{settings.input_transform}}; letter-spacing: {{settings.input_spacing}}; font-weight: {{settings.input_weight}}; width: 100%; overflow: hidden; background: {{settings.input_background}} url("data:image/svg+xml;utf8,%3Csvg%20version%3D%271.0%27%20xmlns%3D%27http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%27%20xmlns%3Axlink%3D%27http%3A%2F%2Fwww.w3.org%2F1999%2Fxlink%27%20viewBox%3D%270%200%2022%2012%27%20enable-background%3D%27new%200%200%2022%2012%27%3E%3Cpolyline%20stroke%3D%27%23000000%27%20fill%3D%27none%27%20stroke-width%3D%273%27%20stroke-linecap%3D%27round%27%20stroke-linejoin%3D%27round%27%20stroke-miterlimit%3D%2710%27%20points%3D%2721%2C1%2011%2C11%201%2C1%27%20%2F%3E%3C%2Fsvg%3E") no-repeat 94% center; background-size: auto 12%; box-shadow: none; -webkit-appearance: none; -moz-appearance: none; appearance: none;} 
select::-ms-expand {display: none;}

/* Input Styles */
input[type='text'],input[type='email'], input[type="tel"], input[type="phone"], input[type='password'], textarea {width: 100%; line-height: 100%; background: {{settings.input_background}}; color: {{settings.input_text}}; border: {{settings.input_border}}; -webkit-border-radius: {{settings.input_radius}}; -moz-border-radius: {{settings.input_radius}}; border-radius: {{settings.input_radius}}; padding: {{settings.input_padding}}; font-size: {{settings.input_size}}; text-transform: {{settings.input_transform}}; letter-spacing: {{settings.input_spacing}}; font-weight: {{settings.input_weight}}; font-family: {{ settings.body_font }}; text-decoration: none; -webkit-transition: all 0.2s linear;  -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear;}
textarea{min-height: 130px;}
input[type='text']:focus, input[type='email']:focus, input[type="tel"]:focus, input[type="phone"]:focus, input[type='password']:focus, textarea:focus {background: {{settings.input_background_focus}}; color: {{settings.input_text_focus}}; border: {{settings.input_border_focus}};}

/* Button Styles */
.btn, a.btn, input.btn, button.btn {display: inline-block; line-height: 100%; background-color: {{settings.button_background}}; color: {{settings.button_text}}; border: {{settings.button_border}}; -webkit-border-radius: {{settings.button_radius}}; -moz-border-radius: {{settings.button_radius}}; border-radius: {{settings.button_radius}}; padding: {{settings.button_padding}}; font-size: {{settings.button_size}}; text-transform: {{settings.button_transform}}; letter-spacing: {{settings.button_spacing}}; font-weight: {{settings.button_weight}}; font-family: {{ settings.body_font }}; font-style: normal; text-align: center; text-decoration: none; cursor: pointer; -webkit-transition: all 0.4s ease-out;  -moz-transition: all 0.4s ease-out; -o-transition: all 0.4s ease-out; -ms-transition: all 0.4s ease-out; transition: all 0.4s ease-out;}
.isNotMobile .btn:hover,.isNotMobile a.btn:hover, .isNotMobile input.btn:hover, .isNotMobile button.btn:hover {background-color: {{settings.button_background_hover}}; color: {{settings.button_text_hover}}; border: {{settings.button_border_hover}}; {% if settings.border_scale_hover %}-ms-transform: scale(1.1, 1.1); -webkit-transform: scale(1.1, 1.1); transform: scale(1.1, 1.1);{% endif %}}

.fa {font-family: 'FontAwesome' !important;}

/**** CUSTOM CHECKBOX AND RADIO STYLES ****/

.checkbox-custom, .radio-custom {opacity: 0; position: absolute;}
.checkbox-custom, .checkbox-custom-label, .radio-custom, .radio-custom-label {display: inline-block; vertical-align: middle; cursor: pointer;}
.checkbox-custom-label, .radio-custom-label { position: relative;}
.checkbox-custom + .checkbox-custom-label:before {float: left; content: ''; background: #fff url({{"checked.png" | asset_url}}) no-repeat center; background-size: 0%; border: 2px solid #e5e5e5; display: inline-block; vertical-align: middle; width: 18px; height: 18px; margin-right: 15px; text-align: center; -webkit-transition: all 0.2s linear; -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear; }
.radio-custom + .radio-custom-label:before {float: left; content: ''; border: 2px solid #e5e5e5; border-radius: 50%; display: inline-block; vertical-align: middle; width: 18px; height: 18px; margin-right: 15px; text-align: center; -webkit-transition: all 0.2s linear; -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear;}
.checkbox-custom:checked + .checkbox-custom-label:before {background-color: #e5e5e5; background-size: 70%;}
.radio-custom + .radio-custom-label:before { border-radius: 50%;}
.radio-custom:checked + .radio-custom-label:before { background: {{settings.content_main_color}}; box-shadow: inset 0px 0px 0px 4px #fff;}

/**** QUANTITY BOX (- 1 +)  ****/

.quantity-box,
.custom-packs-quantity-box {font-size: 1em;}
.quantity-box > div,
.custom-packs-quantity-box > div {align-items: center; max-width: 8em; justify-content: center; padding: 0.2em; border: 1px solid #eee;}
.quantity-box input[type=number]::-webkit-inner-spin-button,
.custom-packs-quantity-box input[type=number]::-webkit-inner-spin-button, 
.quantity-box input[type=number]::-webkit-outer-spin-button,
.custom-packs-quantity-box input[type=number]::-webkit-outer-spin-button {-webkit-appearance: none; margin: 0;}
.quantity-box input[type=number],
.custom-packs-quantity-box input[type=number]{padding: 0; width: 2em; text-align: center; font-weight: 500; color: {{settings.content_text_color}}; border: none; background-color: transparent; -moz-appearance:textfield;}
.quantity-box input.plus,
.custom-packs-quantity-box input.plus,
.quantity-box input.minus,
.custom-packs-quantity-box input.minus {padding: 0; width: 2em; text-align: center; color: {{settings.content_text_color}}; background-color: transparent; border: none;}
.isNotMobile .quantity-box input.plus:hover,
.isNotMobile .custom-packs-quantity-box input.plus:hover,
.isNotMobile .quantity-box input.minus:hover,
.isNotMobile .custom-packs-quantity-box input.minus:hover {color: {{settings.content_main_color}};}

/*
/******************************** SCROLL TO TOP *****************************
*/

.scrollup {display: none; position: fixed; z-index: 90; bottom: 10px; right:10px; }
.scrollup a {font-size:1.6em; background: {{ settings.content_main_color}}; display: block; width: 1.5em; height: 1.5em; padding-top: 0.2em; line-height: 100%; color:#fff; text-align:center; text-decoration:none; border-radius: 0;}
.scrollup i {padding: 0;}
.isNotMobile .scrollup a:hover {color: #fff;}

/*
/******************************** OVERLAYS *****************************
*/

#page::after {position: fixed; top: 0; right: 0; width: 0; height: 0; background: rgba(0,0,0,0.7); content: ''; opacity: 0; -webkit-transition: opacity 0.5s, width 0.1s 0.5s, height 0.1s 0.5s; transition: opacity 0.5s, width 0.1s 0.5s, height 0.1s 0.5s; z-index: 4000;}
#page.mode-overlay::after {width: 100%; height: 100%; opacity: 1; -webkit-transition: opacity 0.5s; transition: opacity 0.5s;}
#loading-overlay {width: 100%; height: 100%; position: fixed; top: 0; left: 0; background:rgba(0,0,0,.7); z-index: 999999; text-align: center; display: none;}
.loader {border: 6px solid rgba(255,255,255,0.2); position: absolute; left: 50%; top: 50%; margin-top: -36px; margin-left: -36px; border-top: 6px solid #fff; border-radius: 50%; width: 60px; height: 60px; animation: spin 0.7s linear infinite; } @keyframes spin {0% { transform: rotate(0deg); } 100% { transform: rotate(360deg); } } @-webkit-keyframes spin {0% { transform: rotate(0deg); } 100% { transform: rotate(360deg); } }


