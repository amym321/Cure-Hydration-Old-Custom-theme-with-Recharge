/* Custom Product Controls */

.product-controls {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 1;
    flex-direction: column;
    opacity: 0;
    transition: opacity .3s ease;
}

.product-controls:hover {
    opacity: 1;
}

.product-controls .btn {
    width: 90%;
    padding: 1.3em 1em;
    margin-bottom: 1em;
}